        <items>
            <VBox alignment="TOP_CENTER" prefHeight="469.0" prefWidth="407.0" spacing="10">
               <children>
                  <ToolBar prefHeight="40.0" prefWidth="200.0">
                     <items>
                        <Button fx:id="addButton" mnemonicParsing="false" onAction="#add" prefWidth="100.0" text="Aggiungi" />
